        <action id="org.diffscope.core.window.&amp;previousProjectWindow" description="Switch to the previous project window" shortcut="Alt+K" />
        <action id="org.diffscope.core.window.pro&amp;jectWindows" diffscope:componentType="menu" />

        <action id="org.diffscope.core.documentations" text="&amp;Help" description="Show help contents" class="Help" icon="question_circle" shortcut="F1" />
