        <action id="org.diffscope.core.timeline.editTempo^" icon="metronome" />
        <action id="org.diffscope.core.timeline.editTimeSignature^" />
        <action id="org.diffscope.core.timeline.editLoop^" icon="arrow_repeat_all" />
        <action id="org.diffscope.core.timeline.enableLoop" icon="arrow_repeat_all" />

        <action id="org.diffscope.core.workspace.workspaceLayout^" />
        <action id="org.diffscope.core.workspace.default&amp;Layout" description="Apply the default workspace layout" diffscope:excludeFromCommands="true" />
        <action id="org.diffscope.core.workspace.&amp;customLayouts" diffscope:componentType="menu" diffscope:excludeFromCommands="true" />
        <action id="org.diffscope.core.workspace.saveLayout" text="&amp;Save Current Layout As..." diffscope:excludeFromCommands="true" />
        <action id="org.diffscope.core.workspace.&amp;allPanels" diffscope:excludeFromCommands="true" />
        <action id="org.diffscope.core.workspace.panelLeftTop" text="None (Left Top)" shortcut="Ctrl+Alt+1" diffscope:excludeFromCommands="true" />
        <action id="org.diffscope.core.workspace.panelLeftBottom" text="None (Left Bottom)" shortcut="Ctrl+Alt+2" diffscope:excludeFromCommands="true" />
        <action id="org.diffscope.core.workspace.panelRightTop" text="None (Right Top)" shortcut="Ctrl+Alt+3" diffscope:excludeFromCommands="true" />
        <action id="org.diffscope.core.workspace.panelRightBottom" text="None (Right Bottom)" shortcut="Ctrl+Alt+4" diffscope:excludeFromCommands="true" />
        <action id="org.diffscope.core.workspace.panelTopLeft" text="None (Top Left)" shortcut="Ctrl+Alt+5" diffscope:excludeFromCommands="true" />
        <action id="org.diffscope.core.workspace.panelTopRight" text="None (Top Right)" shortcut="Ctrl+Alt+6" diffscope:excludeFromCommands="true" />
        <action id="org.diffscope.core.workspace.panelBottomLeft" text="None (Bottom Left)" shortcut="Ctrl+Alt+7" diffscope:excludeFromCommands="true" />
        <action id="org.diffscope.core.workspace.panelBottomRight" text="None (Bottom Right)" shortcut="Ctrl+Alt+8" diffscope:excludeFromCommands="true" />
