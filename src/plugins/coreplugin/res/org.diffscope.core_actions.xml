        <action id="org.diffscope.core.file.metadata^" icon="document_settings" />

        <action id="org.diffscope.core.show&amp;HomeWindow" description="Switch to home window if it is opened, or else open home window" class="Window" icon="home" shortcut="Ctrl+Shift+H" />

