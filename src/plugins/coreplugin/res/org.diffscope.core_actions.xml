        <action id="org.diffscope.core.timeline.goToNextMeasure" shortcut="]" />
        <action id="org.diffscope.core.timeline.goToNextBeat" shortcut="Ctrl+]" />
        <action id="org.diffscope.core.timeline.goToNextTick" shortcut="Ctrl+Shift+]" />
