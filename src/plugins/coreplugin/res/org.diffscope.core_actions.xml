            class="Application::ActionClass"
            description="Application::ActionDescription"
        />
        <vars>
            <var key="APP_NAME" value="DiffScope" />
        </vars>
    </configuration>

    <items>
        <action id="org.diffscope.core.file.&amp;new" description="Create a project in a new window" icon="document_add" shortcut="Ctrl+N" />
        <action id="org.diffscope.core.file.newFromTemplate^" description="Create a project from template in a new window" shortcut="Ctrl+Alt+N" />
