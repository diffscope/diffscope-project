        <action id="org.diffscope.core.view.showToo&amp;lBar" description="Show or hide the main tool bar" />
        <action id="org.diffscope.core.view.show&amp;LeftSideBar" description="Show or hide the left side bar" />
        <action id="org.diffscope.core.view.show&amp;RightSideBar" description="Show or hide the right side bar" />
        <action id="org.diffscope.core.view.show&amp;TopSideBar" description="Show or hide the top side bar" />
        <action id="org.diffscope.core.view.show&amp;BottomSideBar" description="Show or hide the bottom side bar" />
        <action id="org.diffscope.core.view.show&amp;StatusBar" description="Show or hide the status bar" />

