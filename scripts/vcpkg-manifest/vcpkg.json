{
    "$schema": "https://raw.githubusercontent.com/microsoft/vcpkg-tool/main/docs/vcpkg.schema.json",
    "dependencies": [
        "breakpad",
        "qastool",
        "qnrbf",
        "stdutau",
        "extensionsystem",
        "qwindowkit",
        "stdcorelib",
        "qbreakpad",
        "libsndfile",
        "rpclib",
        "rtmidi",
        {
            "name": "sdl2",
            "default-features": false
        },
        "interval-tree",
        "boost-interprocess",
        "wavpack",
        "neargye-semver",
        "r8brain-free-src",
        "portaudio",
        "libsoundio",
        {
            "name": "glib",
            "platform": "linux"
        },
        "wolf-midi"
    ],
    "vcpkg-configuration": {
        "overlay-ports": [
            "../vcpkg/ports"
        ],
        "overlay-triplets": [
            "../vcpkg/triplets"
        ]
    }
}